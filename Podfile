target 'shadowsocks' do
  use_frameworks!
  platform :ios, '9.0'
  pod 'libsodium', '~> 1.0.3'
  pod 'AFNetworking', '~> 2.5.0'
  pod 'ZXingObjC', '= 3.0.0'
  pod 'GCDWebServer', '~> 1.2'
end

target 'ShadowsocksX' do
  use_frameworks!
  platform :osx, '10.10'
  pod 'libsodium', '~> 1.0.3'
  pod 'AFNetworking', '~> 2.5.0'
  pod 'ZXingObjC', '= 3.2.1'
  pod 'GCDWebServer', '~> 1.2'
  pod 'OpenSSL-OSX', '~>1.0.0'
end
